[flake8]

max-line-length = 175
max-complexity = 20
inline-quotes = "

application-import-names = src, tests
import-order-style = pycharm

extend-immutable-calls = Depends, OAuth2PasswordBearer, File, Query, Cookie

exclude = .git, __pycache__, .idea
ignore = ANN101

per-file-ignores =
    scripts/*:T201
